{
  "name": "todo-app-backend",
  "version": "1.0.0",
  "description": "A backend service to create and manage todo list",
  "main": "./src/app.ts",
  "keywords": [
    "database",
    "libpq",
    "pg",
    "postgre",
    "postgres",
    "postgresql",
    "rdbms",
    "sqlite3"
  ],
  "scripts": {
    "lint": "eslint --ext .ts --fix .",
    "build": "npx tsc",
    "start": "set NODE_ENV=development && ts-node src/index.ts",
    "dev": " set NODE_ENV=dev && nodemon src/index.ts",
    "test": "jest --coverage",
    "migration:generate": "npx prisma migrate dev",
    "migration:blank": "npm run typeorm migration:create -n"
  },
  "author": "madeehagh",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
